<launch>

<!-- tf from laser to lidar_frame-->
	<node
		pkg="tf" type="static_transform_publisher" name="laser_link_broadcaster"
		args="0 0 0 0 0 0 lidar_frame laser 100"
	/>
<!-- tf for the top imu to the basse_link of boat-->
	<node
		pkg="tf" type="static_transform_publisher" name="imu2_link_broadcaster"
		args="0 0 0 0 0 0 base_link base_imu2_link 100"
	/>

	<node
		pkg="tf" type="static_transform_publisher" name="base_link_bottom_broadcaster"
		args="0 0 0 0 0 0 lidar_stabilized lidar_frame 100"
	/>

	<node
		pkg="tf" type="static_transform_publisher" name="base_stabilized_broadcaster"
		args="0 0 0 0 0 0 base_link lidar_stabilized 100"
	/>
<!--
	<node
		pkg="tf" type="static_transform_publisher" name="map_broadcaster"
		args="0 0 0 0 0 0 map odom 100"
	/>
-->
</launch>
