  <arg name="razor_config_file" default="$(find master)/params/my_razor.yaml"/>
 <node pkg="razor_imu_9dof" type="imu_node.py" name="lidar_imu_node" output="screen">
    <rosparam file="$(arg razor_config_file)" command="load"/>
  </node>
